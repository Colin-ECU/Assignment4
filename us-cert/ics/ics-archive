<section class="views-element-container block block-views block-views-blockics-alerts-landing-block-2 clearfix" id="block-views-block-ics-alerts-landing-block-2">
  
      <h2 class="block-title">ICS-CERT Alerts</h2>
    

      <div class="form-group"><div class="view view-ics-alerts-landing view-id-ics_alerts_landing view-display-id-block_2 js-view-dom-id-7b10425377c588b7a5d6ab5aea327eda5619d584ddf9d7dd2c94f8dd9946ccc6">
  
    
      
      <div class="view-content">
      <div class="item-list">
  
  <ul>

